<!--pages/withdrawalsList/withdrawalsList.wxml-->
<nav-bar navbar-data='{{nvabarData}}'></nav-bar>
<view class="container">
  <view class="shopContainer">
    <view class="shopNull" style="height:{{height}}px;"></view>
    <view class="companyHead">
      <view class="dateSelect">
          <text>选择日期</text>
          <view class="dateStart">
            <picker mode="date" value="{{dateStart}}" start="{{dateStart}}" end="{{dateEnd}}" bindchange="bindStartChange">
              <view class="picker">
                {{dateStart}}
              </view>
            </picker>
          </view>
          <text>-</text>
          <view class="dateEnd">
            <picker mode="date" value="{{dateEnd}}" start="{{dateStart}}" end="{{dateEnd}}" bindchange="bindEndChange">
              <view class="picker">
                {{dateEnd}}
              </view>
            </picker>
          </view>
          <text class="dateSearch">查询</text>
        </view>
    </view>
    <view class="height20"></view>
    <view class="shopList">
      <view class="listHead">
        <view class="listHeadLeft">
          <view class="lastText">
          <image class="bankImg" src="{{zhaoshangIcon}}"></image>
        招商银行(5468)*刘美琪
          </view>
          <view class="listPrice">3200.00</view>
        </view>
        <view class="listHeadRight">
          <view class="listDate">
            2019/12/02 16:00:00
          </view>
          <view class="revokeType">撤销</view>
        </view>
      </view>
    </view>
    <view class="shopList">
      <view class="listHead">
        <view class="listHeadLeft">
          <view class="lastText">
            <image class="bankImg" src="{{zhaoshangIcon}}"></image>
            <text>招商银行(5468)*刘美琪</text>
          </view>
          <view class="listPrice">3200.00</view>
        </view>
        <view class="listHeadRight">
          <view class="listDate">
            2019/12/02 16:00:00
          </view>
          <view class="haveType">已到账</view>
        </view>
      </view>
    </view>
  </view>
</view>
